{
  "name": "SatVis",
  "version": "1.0.0",
  "description": "Satellite orbit visualization with CesiumJS",
  "scripts": {
    "build": "webpack --config webpack/webpack.prod.js",
    "build:dev": "webpack --config webpack/webpack.config.js",
    "build:stats": "webpack --config webpack/webpack.stats.js",
    "clean": "mkdirp dist & rimraf dist/*",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "lint": "eslint --ext js,vue src",
    "lint:fix": "eslint --ext js,vue --fix src",
    "serve": "npm run build && http-server -c-1 dist",
    "serve:dev": "npm run build:dev && http-server -c-1 dist",
    "serve:stats": "npm run build:stats && http-server -c-1 dist",
    "start": "webpack serve --progress --config webpack/webpack.config.js",
    "start:anyhost": "webpack serve --progress --config webpack/webpack.config.js --host 0.0.0.0 --disable-host-check",
    "test": "ava",
    "update-tle": "data/tle/get-norad.sh"
  },
  "author": "Florian Mauracher",
  "private": true,
  "license": "MIT",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.32",
    "@fortawesome/free-brands-svg-icons": "^5.15.1",
    "@fortawesome/free-solid-svg-icons": "^5.15.1",
    "@sentry/browser": "^5.29.2",
    "buefy": "^0.9.4",
    "cesium": "^1.77.0",
    "cesium-sensor-volumes": "github:Flowm/cesium-sensor-volumes",
    "dayjs": "^1.10.3",
    "satellite.js": "^4.1.3",
    "vue": "^2.6.12",
    "vue-multi-select": "^4.6.0",
    "vue-router": "^3.4.9",
    "workbox-window": "^6.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "autoprefixer": "^10.2.1",
    "ava": "3.15.0",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^7.0.0",
    "css-loader": "^5.0.1",
    "cssnano": "^4.1.10",
    "cypress": "^6.2.1",
    "eslint": "^7.17.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-import-resolver-webpack": "^0.13.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-vue": "^7.4.1",
    "esm": "^3.2.25",
    "html-webpack-plugin": "^5.0.0-beta.4",
    "http-server": "^0.12.3",
    "mini-css-extract-plugin": "^1.3.3",
    "mkdirp": "^1.0.4",
    "obj2gltf": "^3.1.0",
    "postcss-loader": "^4.1.0",
    "rimraf": "^3.0.2",
    "script-loader": "^0.7.2",
    "size-plugin": "^2.0.1",
    "strip-pragma-loader": "^1.0.0",
    "style-loader": "^2.0.0",
    "url-loader": "^4.1.1",
    "vue-loader": "^15.9.6",
    "vue-template-compiler": "^2.6.12",
    "webpack": "^5.12.3",
    "webpack-bundle-analyzer": "^4.3.0",
    "webpack-cli": "^4.3.1",
    "webpack-dev-server": "^3.11.1",
    "webpack-merge": "^5.7.3",
    "workbox-webpack-plugin": "^6.0.2"
  },
  "ava": {
    "require": [
      "esm"
    ]
  },
  "browserslist": [
    "> 1%",
    "not ie 11",
    "not op_mini all",
    "not UCAndroid >1"
  ]
}
